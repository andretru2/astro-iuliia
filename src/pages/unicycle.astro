---
import Layout from "@layouts/Layout.astro";

import { Image } from "@astrojs/image/components";
import { getImage } from "@astrojs/image";
import site from "@src/data/site.js";
import { Sprite } from "astro-icon";
import Hero from "@components/unicycle/Hero.astro";
import ProjectOverview from "@components/unicycle/ProjectOverview.astro";
import CompetitorAnalysis from "@components/unicycle/CompetitorAnalysis.astro";
import UserResearch from "@components/unicycle/UserResearch.astro";
import AffinityDiagram from "@components/unicycle/AffinityDiagram.astro";
import UserInsights from "@components/unicycle/UserInsights.astro";
import EmpathyMap from "@components/unicycle/EmpathyMap.astro";
import UserPersona from "@components/unicycle/UserPersona.astro";
---

<Layout hero={false} class="text-gray-900 text-xl font-poppins">
  <Hero />
  <ProjectOverview />
  <CompetitorAnalysis />
  <UserResearch />
  <AffinityDiagram />
  <UserInsights />
  <EmpathyMap />
  <UserPersona />
</Layout>
