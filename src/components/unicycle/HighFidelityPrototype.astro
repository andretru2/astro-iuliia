---
import SectionTitle from "../SectionTitle.astro";
import Section from "@components/Section.astro";
---

<Section id="high-fidelity-prototype" alternate={false} class="">
  <SectionTitle title1="High Fidelity" title2=" Prototype" />
  <div
    class="mt-6 flex w-full max-w-7xl items-center justify-center rounded-sm bg-slate-50 py-6 px-3 shadow-xl sm:py-10 sm:px-8"
  >
    <video
      controls
      class:list={["video small:preserve3d h-60 sm:h-max sm:max-h-[35rem] "]}
    >
      <source src="images/unicycle-showcase.mp4" type="video/mp4" />
    </video>
  </div>
</Section>
